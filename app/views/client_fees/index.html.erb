<h1>Choose Month</h1>
<div class="change_password_fields">
    <div class="change_password_names">Month</div>
    <div class="change_password_form-field"><input type="text" name="month" id="fee_month" readonly="true" /></div>
    <div class="clr"></div>
  </div>
  

 <div id="client_fee_report">
 	
 </div>  
  <script>
$(function() {



 $("#fee_month").monthpicker().bind('monthpicker-click-month', function (e, month) {
	 e.preventDefault();
	 $.get("client_fees/clients_fee?month="+month,function(data){
	 	  $("#client_fee_report").empty();
	   	  $("#client_fee_report").html(data);
	   	});
    
 });
});
</script>

<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
<script src="../assets/jquery.mtz.monthpicker.js"></script>

